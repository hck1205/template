{
  "compilerOptions": {
    "jsx": "react",
    "types": ["react", "node"],
    "target": "es5",
    "strict": true,
    "lib": ["es2015", "es2016", "es2017", "es2018", "es2019", "es2020", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./build",
    "module": "esnext",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "baseUrl": "src",
    "paths": {
      "components/*": ["components/*"],
      "pages/*": ["pages/*"],
      "lib/*": ["lib/*"],
      "styles/*": ["lib/styles/*"],
      "API/*": ["API/*"],
      "assets/*": ["assets/*"],
      "constpack/*": ["constpack/*"],
      "stores/*": ["stores/*"]
    },
    "typeRoots": ["types", "../node_modules/@types"]
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "build"]
}
